            <geom type="box" size="0.023 0.023 .49" pos="-.5265 0.3447 0.49" material="vention_blue"/>
            <geom type="box" size="0.023 0.023 .49" pos="0.5265 0.3447 0.49" material="vention_blue"/>

            <!-- middle legs -->
            <geom type="box" size="0.023 0.023 .49" pos="0 0.3393 0.49" material="vention_blue"/>
            <geom type="box" size="0.023 0.023 .49" pos="0 -.3393 0.49" material="vention_blue"/>

            <!-- top board -->
            <geom type="box" size="0.5526 0.3699 0.023" pos="0 0 0.977" material="counter_marble"/>
            <geom type="box" size="0.5508 0.3681 0.023" pos="0 0 0.966" material="counter_marble"/>

            <!-- bottom board -->
            <geom type="box" size="0.5418 0.3591 0.005" pos="0 0 0.150" material="vention_black"/>
            <geom type="box" size="0.5436 0.3609 0.023" pos="0 0 0.124" material="vention_blue"/>

            <!-- collision -->
