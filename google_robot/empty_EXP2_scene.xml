        <geom name="geo7" class="visual" mesh="009" material="009_mtl" rgba="1 1 1 0"/>
        <geom name="geo8" class="visual" mesh="013" material="013_mtl" rgba="1 1 1 0"/>
        <geom name="geo9" class="visual" mesh="008" material="008_mtl" rgba="1 1 1 0"/>
        <geom name="geo10" class="visual" mesh="013" material="013_mtl" rgba="1 1 1 0"/>
